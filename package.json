{
  "name": "typescript-datastructures",
  "version": "1.0.0",
  "description": "Some data structures implemented in TypeScript, as exercises to learn the language ",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "chai": "^4.1.2",
    "install": "^0.10.4",
    "mocha": "^5.0.4",
    "npm": "^5.7.1",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.7.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  },
  "scripts": {
    "compile-bst": "tsc -p ./binary-search-tree/tsconfig.json",
    "test-bst": "mocha -r ts-node/register ./binary-search-tree/**/*.spec.ts",
    "pack-bst": "webpack --config ./binary-search-tree/webpack.config.js",
    "docs-bst": "typedoc --out binary-search-tree/docs/ --tsconfig binary-search-tree/tsconfig.json --excludePrivate --exclude **/**/demo/**/*.ts",
    "build-bst": "npm run compile-bst && npm run test-bst && npm run pack-bst",

    "compile-ntree": "tsc -p ./n-tree/tsconfig.json",
    "test-ntree": "mocha -r ts-node/register ./n-tree/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JeffersonCarvalh0/typescript-datastructures.git"
  },
  "author": "Jefferson Carvalho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JeffersonCarvalh0/typescript-datastructures/issues"
  },
  "homepage": "https://github.com/JeffersonCarvalh0/typescript-datastructures#readme"
}
